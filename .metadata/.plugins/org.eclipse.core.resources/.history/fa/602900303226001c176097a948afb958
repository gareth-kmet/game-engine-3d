package game.world;

public class WorldRandomStyles {
	
	private static enum RandomStyles{
		LAND_HEIGHT(0b00001),
		WATER_DEPTH(0b00010),
		
		;
		public final byte id;
		public WorldStyle style = null;
		
		private RandomStyles(int id){
			this.id=(byte)id;
		}
		
		
		
	}
	
	public static interface WorldStyle{
		
	}

}
